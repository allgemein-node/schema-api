  "directories": {
    "doc": "docs/de"
  },
  "scripts": {
    "test": "nyc mocha --opts test/mocha.all.opts",
    "posttest": "nyc report --reporter=json && codecov -f coverage/*.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allgemein-node/schema-api.git"
  },
  "author": "Robert Kania",
  "license": "MIT",
  "keywords": [
    "commons",
    "typescript",
    "schema",
    "api"
  ],
  "bugs": {
    "url": "https://github.com/allgemein-node/schema-api/issues"
  },
  "homepage": "https://github.com/allgemein-node/schema-api#readme",
  "devDependencies": {
    "@allgemein/packaging": "^1.1.0",
    "@testdeck/mocha": "^0.1.0",
    "@types/chai": "^4.2.12",
    "@types/lodash": "^4.14.159",
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.0.27",
    "chai": "^4.2.0",
    "codelyzer": "^6.0.0",
    "del": "5.1.0",
    "gulp": "4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-clean": "^0.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-shell": "0.8.0",
    "gulp-sourcemaps": "2.6.5",
    "gulp-typescript": "5.0.1",
    "gulpclass": "0.2.0",
    "merge-stream": "^2.0.0",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "@allgemein/base": "^1.3.0",
    "reflect-metadata": "^0.1.13"
  },
  "private": true,
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "tmp/",
      "build/"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false
  }
